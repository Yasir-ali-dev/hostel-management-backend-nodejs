{
    "version": 2,
    "builds": [
      {
        "src": "./index.js", 
        "use": "@vercel/node",
        "config": {
          "memory": 512,
          "maxDuration": 10,
          "timeout": 30
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
    
}
  