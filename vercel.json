{
    "version": 2,
    "builds": [
      {
        "src": "./index.js", 
        "use": "@vercel/node",
        "config": {
          "memory": 812,
          "maxDuration": 20,
          "timeout": 60
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
    
}
  