{
  "functions": {
    "api/*": {
      "runtime": "nodejs14.x",
      "memory": 512, 
            "maxDuration": 10, 
      "timeout": 30 
    }
  },
    "version": 2,
    "builds": [
      {
        "src": "./index.js", 
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
}
  